@model IEnumerable<SmartActS.DataModels.Rank>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-bordered" cellpadding="0" width="100%">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RankCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RankName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RankCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RankName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.RankId }) |
                @Html.ActionLink("Details", "Details", new { id = item.RankId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.RankId })
            </td>
        </tr>
    }

</table>
